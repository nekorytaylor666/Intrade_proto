    "text-report": "cross-env NODE_ENV=test nyc --reporter=text-summary mocha --timeout 5000 __tests__/server/**/*.test.js",
    "nyc": "cross-env NODE_ENV=test nyc mocha --timeout 5000 __tests__/server/**/*.test.js"
  },
  "author": "CodeWorkr",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "concurrently": "^4.1.2",
    "cross-env": "^5.0.5",
    "express": "^4.15.3",
    "express-promise-router": "^2.0.0",
    "joi": "^10.6.0",
    "jsonwebtoken": "^7.4.1",
    "mongoose": "^5.7.5",
    "morgan": "^1.8.2",
    "passport": "^0.3.2",
    "passport-google-plus-token": "^2.1.0",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-form": "^8.2.6"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "faker": "^4.1.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.11.0",
